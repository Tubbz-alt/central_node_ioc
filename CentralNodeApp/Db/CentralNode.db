record(stringout, "CentralNode:CONFIG_LOAD") {
    field(DESC, "Load Algorithm")
    field(DTYP, "asynOctetWrite")
    field(OUT, "@asyn(CENTRAL_NODE 0 0)CONFIG_LOAD")
}

record(stringout, "CentralNode:Test:DEVICE_INPUT") {
    field(DESC, "Load values for device inputs (digital)")
    field(DTYP, "asynOctetWrite")
    field(OUT, "@asyn(CENTRAL_NODE 0 0)TEST_DEVICE_INPUT")
}

record(stringout, "CentralNode:Test:ANALOG_DEVICE") {
    field(DESC, "Load test file for analog devices")
    field(DTYP, "asynOctetWrite")
    field(OUT, "@asyn(CENTRAL_NODE 0 0)TEST_ANALOG_DEVICE")
}

record(stringin, "CentralNode:FW_BUILD_STAMP") {
    field(DESC, "Central node firmware build stamp")
    field(DTYP, "asynOctetRead")
    field(SCAN, "1 second")
    field(INP, "@asyn(CENTRAL_NODE 0 0)FW_BUILD_STAMP_STRING")
}

record(longout, "CentralNode:Test:CHECK_FAULTS") {
    field(DESC, "Run engine evaluation one time")
    field(DTYP, "asynInt32")
    field(VAL, "0")
    field(PINI, "YES")
    field(OUT, "@asyn(CENTRAL_NODE 0 0)TEST_CHECK_FAULTS")
}

record(longout, "CentralNode:Test:CHECK_BYPASS") {
    field(DESC, "Check bypass queue one time")
    field(DTYP, "asynInt32")
    field(OUT, "@asyn(CENTRAL_NODE 0 0)TEST_CHECK_BYPASS")
}

record(longout, "CentralNode:UNLATCHALL") {
    field(DESC, "Unlatch all faults and inputs")
    field(DTYP, "asynInt32")
    field(OUT, "@asyn(CENTRAL_NODE 0 0)UNLATCH_ALL")
}